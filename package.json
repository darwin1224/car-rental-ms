{
  "name": "car-rental-ms",
  "version": "0.1.0",
  "description": "Car Rental Management System",
  "author": "Darwin",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "docs": "compodoc -p tsconfig.json -d docs/references -s",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose --config ./test/jest-e2e.json",
    "heroku-postbuild": "yarn schema:drop -f ormconfig.seed.js && yarn schema:sync -f ormconfig.seed.js && yarn seed:run -s UserSeed -n ormconfig.seed.js && yarn build"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "nestjs-typeorm-paginate": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/faker": "^4.1.12",
    "@types/helmet": "^0.0.47",
    "@types/jest": "25.2.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
